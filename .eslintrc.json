{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "max-len": [1, { "code": 100 }],
    "import/no-extraneous-dependencies": 0
  },
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "plugins": ["jest"]
}
